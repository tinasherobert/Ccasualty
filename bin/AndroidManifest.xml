<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="it.sephiroth.android.library.floatingmenu.app">
 <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />
	<application
		android:allowBackup="true"
		android:icon="@drawable/ic_launcher"
		android:label="@string/app_name"
		android:hardwareAccelerated="true"
		android:largeHeap="true"
		android:theme="@style/AppTheme">
		<activity
			android:name="it.sephiroth.android.library.floatingmenu.app.MainActivity"
			android:label="@string/title_activity_main_activity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name="it.sephiroth.android.library.floatingmenu.app.MainActivity2"
			android:label="@string/title_activity_main_activity2"></activity>
		
		<activity
			android:name="it.sephiroth.android.library.floatingmenu.app.TabActivity1"
			></activity>
		<activity
			android:name="it.sephiroth.android.library.floatingmenu.app.TabActivity3"
			></activity>
		<activity
			android:name="it.sephiroth.android.library.floatingmenu.app.TabActivity2"
			></activity>
		
		<activity
			android:name="it.sephiroth.android.library.floatingmenu.app.ScrollViewActivity"
			android:icon="@drawable/ic_location_map"
            android:label="@string/new_york_city"
            android:theme="@style/AppTheme.TranslucentActionBar"></activity>
	</application>

</manifest>
